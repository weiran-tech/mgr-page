{
    "name": "weiran/mgr-page",
    "description": "An admin management page module.",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Mark Zhao",
            "email": "zhaody901@126.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "mews/captcha": "^3.3",
        "weiran/system": "1.0.x-dev"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Weiran\\MgrPage\\": "src/"
        },
        "files": [
            "src/Support/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Weiran\\MgrPage\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "laravel": {
        "aliases": {
            "Form": "Weiran\\MgrPage\\Facade\\FormFacade"
        }
    },
    "minimum-stability": "dev"
}
